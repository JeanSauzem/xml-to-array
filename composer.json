{
    "name": "metawesome/laravel-xml-to-array",
    "description": "Easily convert valid xml to a php array.",
    "keywords": [
        "laravel",
        "xml",
        "array",
        "convert"
    ],
    "authors": [
       
    ],
	"autoload": {
        "psr-4": {
            "Metawesome\\XmlToArray\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "~7.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^6.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Metawesome\\XmlToArray\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
